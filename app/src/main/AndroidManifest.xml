<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lenovo.newgame">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.HomeActivity"
            android:screenOrientation="userPortrait" />
        <activity
            android:name=".activity.Main2Activity"
            android:label="Learn"
            android:parentActivityName=".activity.HomeActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.HomeActivity" />
        </activity>

        <activity
            android:name=".activity.TrignoActivity"
            android:screenOrientation="userPortrait" />
        <activity
            android:name=".activity.basic"
            android:screenOrientation="userPortrait" />
        <activity
            android:name=".activity.object"
            android:screenOrientation="userPortrait" />
        <activity android:name=".activity.Scoreborad" />
        <activity android:name=".activity.Dashboard_Activity" />
        <activity android:name=".activity.game_2048" />
        <activity android:name=".quiz" />
        <activity android:name=".activity.video">

        </activity>
    </application>

</manifest>